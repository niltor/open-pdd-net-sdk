<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net452</TargetFrameworks>
    <LangVersion>7.1</LangVersion>
    <RootNamespace>MSDev.PddOpenSdk</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>MSDev_NilTor</Authors>
    <Company>Geethin</Company>
    <Description>
      拼多多SDK核心类库
      已更新同步到最新！
    </Description>
    <PackageReleaseNotes>
      拼多多SDK核心类库
      已更新同步到最新！
    </PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/niltor/open-pdd-net-sdk</PackageProjectUrl>
    <RepositoryUrl>https://github.com/niltor/open-pdd-net-sdk</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>pdd,net core,sdk,拼多多</PackageTags>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <AssemblyVersion>0.1.0.0</AssemblyVersion>
    <FileVersion>0.1.0.0</FileVersion>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/niltor/open-pdd-net-sdk/dev/logo.ico</PackageIconUrl>
    <Version>1.1.4</Version>
    <PackageId>MSDev.PddOpenSdk</PackageId>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <Copyright />
  </PropertyGroup>

  <ItemGroup Condition="'$(TargetFramework)' == 'net452'">
    <Reference Include="System" />
    <Reference Include="System.Net.Http" />
    <Reference Include="System.Data" />
    <Reference Include="System.Xml" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
  </ItemGroup>

</Project>
